		"@sveltejs/kit": "^1.30.2",
		"dexie": "4.0.1-beta.5",
		"dexie-cloud-addon": "4.0.1-beta.48",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
